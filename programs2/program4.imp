[ program4.imp ]
DECLARE
	b, c, j, t
BEGIN
	j ASSIGN -111110 MINUS -111111;
	READ b;
	READ c;
	IF b GE 50 THEN
		b ASSIGN b MOD -50;
        b ASSIGN b PLUS 49;
	ENDIF
	t ASSIGN b PLUS 1000;
	t ASSIGN t MINUS 1;
	WRITE t;
	FOR i FROM b TO t DO
		t ASSIGN i DIV j;
		c ASSIGN c PLUS t;
		t ASSIGN i MOD i;
		c ASSIGN c PLUS t;
	ENDFOR
	WRITE t;
	WRITE c;
END

