[ program5.imp ]
DECLARE
  tab(-499999999:499999999), a, b, c, d, n
BEGIN
  READ a;
  tab(a) ASSIGN 9;

  n ASSIGN 499999999;
  tab(n) ASSIGN tab(a);

  b ASSIGN  111111111;
  b ASSIGN b TIMES tab(a);

  c ASSIGN 90909090;
  READ d;

  tab(a) ASSIGN b MINUS c;
  tab(n) ASSIGN tab(a) DIV d;
  WRITE tab(a);
  WRITE tab(n);
END

